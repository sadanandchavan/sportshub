{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lightable": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/lightable",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              }
            ],
            "styles": [
              "src/assets/fonts/tabler-icons.min.css",
              "src/assets/fonts/feather.css",
              "src/assets/fonts/fontawesome.css",
              "src/assets/fonts/material.css",
              "src/assets/css/uikit.css",
              "src/assets/css/plugins/animate.min.css",
              "src/assets/css/plugins/jsvectormap.min.css",
              "src/assets/css/plugins/nouislider.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/flatpickr/dist/flatpickr.min.css",
              "node_modules/swiper/swiper-bundle.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/animate.css/animate.min.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/bootstrap-switch-button/css/bootstrap-switch-button.css",
              "node_modules/ngx-lightbox/lightbox.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/prismjs/themes/prism.css",
              "node_modules/croppr/dist/croppr.min.css",
              "src/assets/css/plugins/dataTables.bootstrap5.min.css",
              "src/assets/css/plugins/bootstrap-switch-button.min.css",
              "src/assets/css/plugins/quill.snow.css",
              "src/assets/css/style.css",
              "src/assets/css/style-preset.css",
              "src/assets/fonts/phosphor/duotone/style.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/bootstrap-switch-button/dist/bootstrap-switch-button.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/jsvectormap/dist/jsvectormap.min.js",
              "node_modules/jsvectormap/dist/maps/world.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-css.min.js"
            ],
            "server": "src/main.server.ts",
            "prerender": false,
            "ssr": false,
            "allowedCommonJsDependencies": [
              "quill-delta",
              "jsvectormap",
              "apexcharts",
              "choices.js",
              "croppr",
              "@ckeditor/ckeditor5-build-classic",
              "file-saver",
              "prismjs",
              "clipboard",
              "masonry-layout",
              "sweetalert2"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "30kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "lightable:build:production"
            },
            "development": {
              "buildTarget": "lightable:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "fe6fcfdf-2bcf-4413-b957-c46086bb463d"
  }
}
